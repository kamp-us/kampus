{
	"name": "kamp-us",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "tsc -b && vite build",
		"lint": "eslint .",
		"typecheck": "tsc --noEmit",
		"preview": "pnpm run build && vite preview",
		"deploy": "pnpm run build && wrangler deploy",
		"cf-typegen": "wrangler types",
		"relay": "relay-compiler",
		"relay:watch": "relay-compiler --watch",
		"schema:fetch": "curl -s http://localhost:8787/graphql/schema > schema.graphql"
	},
	"volta": {
		"extends": "../../package.json"
	},
	"dependencies": {
		"@base-ui/react": "catalog:",
		"@effect-atom/atom": "catalog:",
		"@effect-atom/atom-react": "catalog:",
		"@effect/platform": "catalog:",
		"@effect/rpc": "catalog:",
		"@kampus/ghostty-react": "workspace:*",
		"@kampus/library": "workspace:*",
		"@radix-ui/colors": "catalog:",
		"@usirin/forge": "catalog:",
		"@usirin/layout-tree": "catalog:",
		"dompurify": "catalog:",
		"effect": "catalog:",
		"ghostty-web": "catalog:",
		"react": "^19.1.1",
		"react-dom": "^19.1.1",
		"react-relay": "catalog:",
		"react-resizable-panels": "catalog:",
		"react-router": "catalog:",
		"relay-runtime": "catalog:",
		"shiki": "catalog:"
	},
	"devDependencies": {
		"@cloudflare/vite-plugin": "catalog:",
		"@eslint/js": "^9.33.0",
		"@swc/plugin-relay": "catalog:",
		"@types/dompurify": "catalog:",
		"@types/react": "^19.1.10",
		"@types/react-dom": "^19.1.7",
		"@types/react-relay": "catalog:",
		"@types/relay-runtime": "catalog:",
		"@vitejs/plugin-react-swc": "^4.0.0",
		"eslint": "^9.33.0",
		"eslint-plugin-react-hooks": "^5.2.0",
		"eslint-plugin-react-refresh": "^0.4.20",
		"globals": "^16.3.0",
		"relay-compiler": "catalog:",
		"typescript": "catalog:",
		"typescript-eslint": "^8.39.1",
		"vite": "^7.1.2",
		"wrangler": "catalog:"
	}
}
